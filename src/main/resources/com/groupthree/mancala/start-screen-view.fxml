<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.Button?>
<VBox alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.groupthree.mancala.controllers.StartController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>

    <Label fx:id="applicationName">
        Mancala
    </Label>
    <Label fx:id="groupName">
        Group 3
    </Label>
    <Button text="Login" onAction="#onLoginButtonClick"/>
    <Button text="Sign Up" onAction="#onSignUpButtonClick"/>
</VBox>
