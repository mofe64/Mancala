      <Label alignment="CENTER" layoutX="237.0" layoutY="65.0" prefHeight="35.0" prefWidth="127.0" text="Welcome">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <Label layoutX="212.0" layoutY="134.0" text="Username" />
      <Group layoutX="212.0" layoutY="296.0">
         <children>
            <Label layoutY="3.0" text="Don't have an account, sign up" />
            <Hyperlink layoutX="164.0" onAction="#goToRegister" text="here" />
         </children>
      </Group>
   </children>
